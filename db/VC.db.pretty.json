        "word": {
            "mayselect": true,
            "featuresetting": {
                "frequency_rank": {
                    "ignoreShow": true,
                    "ignoreRequest": true,
                    "isRange": true
                },
                "lexeme_occurrences": {
                    "ignoreShow": true,
                    "ignoreRequest": true,
                    "isRange": true
                },
                "ref" : {
                    "ignoreSelect": true,
                    "ignoreShow": true,
                    "ignoreRequest": true
                },
                "surface": {
                    "hideWord": true,
                    "foreignText": true
                },
                "suffix" : {
                    "ignoreSelect": true,
                    "ignoreShow": true,
                    "ignoreRequest": true
                },
                "normalized": {
                    "hideWord": true,
                    "foreignText": true
                },
                "lemma": {
                    "foreignText": true
                },
                "lemma_variant": {
                    "ignoreShow": true,
                    "ignoreRequest": true
                },
                "lemma_with_variant": {
                    "foreignText": true
                },
                "psp": {
                    "isDefault": true
                },
